
class Node{
	int data;
	Node next;
	//constructor
	Node(int data){
		this.data=data;
	}
} 

public class SumLists {

	
	public static Node createDummyList(){
		int input[] ={7,1,6};
		Node head=new Node(input[0]);
		Node temp=head;
		for(int i=1;i<input.length;i++){
			Node node=new Node(input[i]);
			temp.next=node;
			temp=temp.next;
		}
		temp=head;
		while(temp!=null){
			System.out.println(temp.data);
			temp=temp.next;
		}
		return head;
	}
	
	
}



