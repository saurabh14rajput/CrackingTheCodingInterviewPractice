package hash.table.imple;
class Node{
	String key;
	int value;
}


public class HashTable {
	Node hashArray[]=new Node[10000];

	public int hashFunction(String input){
		int hashCode = 7;
		for (int i = 0; i < input.length(); i++) {
			hashCode= hashCode*31 + input.charAt(i);
		}
		return hashCode;
	}
	
	public int getHashArrayIndex(int hashCode){
		return hashCode % hashArray.length;
	}
}
