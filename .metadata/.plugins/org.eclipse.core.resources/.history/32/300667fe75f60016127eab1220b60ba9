package is.permutsation.example;

import java.util.Arrays;
import java.util.Scanner;

public class IsPermutation {
	public static void main(String []args){
		Scanner sc = new Scanner(System.in);
		String input1=sc.next() ;
		System.out.println(input1);
		String input2=sc.next();
		System.out.println(input2);
		
		if(isPermutation(input1,input2)){
			System.out.println("Yes");
		}
		else{
			System.out.println("No");
		}
			
		
		
		char in1[]=input1.toCharArray();
		Arrays.sort(in1);
		input1=in1.toString();
		System.out.println(in1);
		char in2[]=input2.toCharArray();
		Arrays.sort(in2);
		input2=in2.toString();
		System.out.println(in2);
		System.out.println(input1.compareTo(input2));
		if(input1.compareTo(input2)==0)
			System.out.println("it is a permutation");
		else
			System.out.println("it is not a permutation");
		
		
	}
	
	public boolean isPermutation(String input1,String input2){
		
		return true;
	}
}
